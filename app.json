{
  "expo": {
    "name": "moongraphy",
    "slug": "moongraphy",
    "scheme": "moongraphy",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0D0F14"
    },
    "updates": { "fallbackToCacheTimeout": 0 },
    "assetBundlePatterns": ["**/*"],
    "ios": { "supportsTablet": true },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0F14"
      },
      "package": "com.moongraphy.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": []
  }
}
